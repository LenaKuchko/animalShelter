<h1>Shelter</h1>
<hr />
<h2>Species:</h2>
<ul>
  @if (Model.Count != 0)
  {
    @foreach (var species in Model)
    {
      <li><a href="/species/@species.GetSpeciesId()">@species.GetType()</a> | <a href="/species/edit/@species.GetSpeciesId()">Edit</a></li>
    }
  }
</ul>
<h4><a href="/species/new">Add a new species</a></h4>
<h4><a href="/">Return home</a></h4>

<form action="/species/clear" method="post">
  <button type="submit">Clear all categories</button>
</form>
